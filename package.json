{
  "name": "swell-scope",
  "version": "1.0.0",
  "description": "Advanced restaking analytics and risk management platform for Swellchain",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "backend:dev": "nodemon backend/src/index.ts",
    "backend:build": "tsc -p backend/tsconfig.json",
    "backend:start": "node backend/dist/index.js",
    "contracts:build": "forge build",
    "contracts:test": "forge test",
    "contracts:deploy": "forge script script/Deploy.s.sol",
    "contracts:verify": "forge verify-contract",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t swell-scope .",
    "docker:run": "docker run -p 3000:3000 swell-scope",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "npm run build && npx @next/bundle-analyzer"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.4",
