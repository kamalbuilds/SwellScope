{
  "name": "swell-scope",
  "version": "1.0.0",
  "description": "Advanced restaking analytics and risk management platform for Swellchain",
  "main": "index.js",
  "scripts": {
    "dev": "npm run backend:dev",
    "build": "npm run backend:build && npm run frontend:build",
    "start": "npm run backend:start",
    "lint": "eslint backend/src --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "backend:dev": "nodemon backend/src/index.ts",
    "backend:build": "tsc -p backend/tsconfig.json",
    "backend:start": "node backend/dist/index.js",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:start": "cd frontend && npm start",
    "contracts:build": "cd contracts && forge build",
    "contracts:test": "cd contracts && forge test",
    "contracts:deploy": "cd contracts && forge script script/Deploy.s.sol",
    "contracts:verify": "cd contracts && forge verify-contract",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t swell-scope .",
    "docker:run": "docker run -p 3000:3000 swell-scope",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "socket.io": "^4.7.4",
    "viem": "^1.19.11",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swellscope/swell-scope.git"
  },
  "keywords": [
    "swellchain",
    "restaking",
    "defi",
    "analytics",
    "risk-management",
    "liquid-staking",
    "ethereum",
    "boring-vault",
    "avs"
  ],
  "author": "SwellScope Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swellscope/swell-scope/issues"
  },
  "homepage": "https://swellscope.xyz"
}
